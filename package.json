{
  "name": "@noom/community-web-uikit",
  "version": "2.14.32",
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
