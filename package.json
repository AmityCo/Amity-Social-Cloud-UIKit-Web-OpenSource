{
  "name": "@noom/community-web-uikit",
  "version": "2.23.0",
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "module": "build/index.js",
  "files": [
    "build"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "description": "React Ui-Kit for Amity Social Cloud (open-source)",
  "scripts": {
    "clean": "rimraf build/*",
    "start": "npm run storybook",
    "build": "npm run clean && npm run build:js",
    "build:js": "tsc && tsc-alias",
    "build:types": "tsc --emitDeclarationOnly",
    "watch": "npm run clean && tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "build:dev:analyze": "webpack --mode development --analyze true",
    "build:prod:analyze": "webpack --mode production --analyze true",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook --output-dir ./storybook-build",
    "lint": "eslint ./src/**/*.{js,ts,tsx}",
    "lint:fix": "npm run lint -- --fix",
    "release": "release-it",
    "release:automatic": "release-it --ci",
    "release:pre": "release-it --preRelease=alpha",
    "format": "prettier --config ./.prettierrc.js --check 'src/**/*'"
  },
  "peerDependencies": {
    "@noom/wax-component-library": ">=4.15.2",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2",
    "react-router-dom": ">=6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@release-it/conventional-changelog": "^5.1.0",
    "@storybook/addon-a11y": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/client-api": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/theming": "^6.5.9",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.4.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-eko": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "2.4.0",
    "recoil": "^0.7.5",
    "release-it": "^15.4.1",
    "svg-url-loader": "^7.1.1",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.6.2",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@amityco/js-sdk": "5.29.0",
    "@hookform/error-message": "2.0.0",
    "@udecode/plate": "14.2.0",
    "classnames": "2.3.1",
    "filesize": "9.0.11",
    "hls.js": "1.2.1",
    "html-entities": "2.3.3",
    "human-readable-numbers": "0.9.5",
    "install": "0.13.0",
    "is-hotkey": "0.2.0",
    "javascript-time-ago": "2.5.6",
    "js-abbreviation-number": "^1.4.0",
    "lodash": "4.17.21",
    "lodash.isequal": "4.5.0",
    "markdown-to-jsx": "7.1.7",
    "polished": "4.2.2",
    "prop-types": "15.8.1",
    "react-dnd": "16.0.1",
    "react-helmet": "6.1.0",
    "react-hook-form": "7.34.2",
    "react-icons": "4.4.0",
    "react-infinite-scroll-component": "6.1.0",
    "react-infinite-scroller": "1.2.6",
    "react-intl": "6.0.5",
    "react-intl-formatted-duration": "4.0.0",
    "react-linkify": "1.0.0-alpha",
    "react-loading-skeleton": "3.1.0",
    "react-mentions": "4.4.7",
    "react-sizeme": "3.0.2",
    "react-textarea-autosize": "8.3.4",
    "react-timeago": "7.1.0",
    "react-tiny-popover": "7.1.0",
    "react-truncate-markup": "5.1.2",
    "react-use": "17.4.0",
    "recoil": "0.0.10",
    "remark-gfm": "3.0.1",
    "remark-parse": "10.0.1",
    "remark-slate": "1.8.6",
    "slate": "^0.82.0",
    "slate-history": "0.66.0",
    "slate-react": "0.82.0",
    "styled-components": "5.3.5",
    "unified": "10.1.2",
    "uuid": "^8.3.2"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --cache --fix",
    "*.{json,md}": "prettier --write"
  },
  "license": "LGPL-2.1-only"
}
